contract ETF is ERC20, Ownable {
    //0.1 eth.
    uint256 minimum_amount = 100000000000;

    IPancakeRouter02 public pancakeRouter;

    address token1;
    address token2;

    address router_address;

    //0.5% Fee on buys and sells.
    uint256 public owner_fee = 5;
    uint256 public fee_percentage_decimals = 3;

    uint256 public token1amount = 50;
    uint256 public token2amount = 50;

    receive() external payable {}

    constructor(
        string memory _name,
        string memory _symbol,
        address _router,
        address _token1,
        address _token2
    ) ERC20(_name, _symbol) {
        pancakeRouter = IPancakeRouter02(_router);
        router_address = _router;
        token1 = _token1;
        token2 = _token2;
    }

    //The ETF is some amount of tokens.
    //When tokens are bought, it is calculated how much of every token contract got
    //and proportional amount of etf is minted to user.
    function mint() public payable {
        require(msg.value >= minimum_amount, "Buying amount is too low.");

        uint256 balance1 = ERC20(token1).balanceOf(address(this));
        uint256 balance2 = ERC20(token2).balanceOf(address(this));

        uint256 fee = (msg.value * owner_fee) / 10**fee_percentage_decimals;
        uint256 deadline = block.timestamp + 15;

        //Requires minimum received tokens
        uint256[2] memory prices = getTokenPrice();
        uint256 token1share = (prices[0] * 100) / (prices[0] + prices[1]);
        uint256 token2share = 100 - token1share;
        uint256 token1value = ((msg.value * token1share) / 100) - (fee / 2);
        uint256 token2value = ((msg.value * token2share) / 100) - (fee / 2);

        uint256 tokensGot1 = getMinimumTokens(
            token1value,
            getTokenPath(token1)
        );
        uint256 tokensGot2 = getMinimumTokens(
            token2value,
            getTokenPath(token2)
        );
        require(msg.value >= token1value + token2value, "aaa");
        //2% slippage
        pancakeRouter.swapExactETHForTokens{value: token1value}(
            (tokensGot1 * 98) / 100,
            getTokenPath(token1),
            address(this),
            deadline
        );
        pancakeRouter.swapExactETHForTokens{value: token2value}(
            (tokensGot2 * 98) / 100,
            getTokenPath(token2),
            address(this),
            deadline
        );

        uint256 userBalance1 = ERC20(token1).balanceOf(address(this)) -
            balance1;
        uint256 userBalance2 = ERC20(token2).balanceOf(address(this)) -
            balance2;

        uint256 etfShare1 = (userBalance1 / token1amount) * 100;
        uint256 etfShare2 = (userBalance2 / token2amount) * 100;
        if (etfShare1 <= etfShare2) {
            _mint(msg.sender, etfShare1 / 100);
        } else {
            _mint(msg.sender, etfShare2 / 100);
        }
    }

    function burn(uint256 _amount) public {
        require(_amount >= minimum_amount / 10, "selling amount is too low.");
        require(
            IERC20(address(this)).transferFrom(
                msg.sender,
                address(this),
                _amount
            )
        );
        uint256 currentBalance = address(this).balance;

        address[] memory path1 = getTokenPathReverse(token1);
        address[] memory path2 = getTokenPathReverse(token2);

        uint256 minimumTokens1 = (getMinimumTokens(_amount, path1) * 98) / 100;
        uint256 minimumTokens2 = (getMinimumTokens(_amount, path2) * 98) / 100;

        IERC20(token1).approve(router_address, token1amount * _amount);
        IERC20(token2).approve(router_address, token2amount * _amount);

        pancakeRouter.swapExactTokensForETH(
            token1amount * _amount,
            minimumTokens1,
            path1,
            address(this),
            block.timestamp + 15
        );
        pancakeRouter.swapExactTokensForETH(
            token2amount * _amount,
            minimumTokens2,
            path2,
            address(this),
            block.timestamp + 15
        );

        uint256 usersETH = (address(this).balance - currentBalance);
        uint256 fee = (usersETH * owner_fee) / 10**fee_percentage_decimals;

        _burn(address(this), _amount);

        (bool success, ) = msg.sender.call{value: usersETH - fee}("");
        require(success, "transfer failed");
    }

    //amount in eth
    function getTokenPrice() public view returns (uint256[2] memory) {
        address[] memory path1 = getTokenPath(token1);
        address[] memory path2 = getTokenPath(token2);
        //token1price + token2price = ETF price
        //When tokens inputed, returns how much eth it will cost.
        uint256[] memory token1price = pancakeRouter.getAmountsIn(
            token1amount,
            path1
        );
        uint256[] memory token2price = pancakeRouter.getAmountsIn(
            token2amount,
            path2
        );
        return [token1price[0], token2price[0]];
    }

    function getMinimumTokens(uint256 _amount, address[] memory _path)
        public
        view
        returns (uint256)
    {
        //With eth inputed, returns token price.
        //token1price1 + token2price2 = how much tokens, approximately user will get considering eth amount.
        uint256[] memory tokensGot = pancakeRouter.getAmountsOut(
            _amount,
            _path
        );

        return tokensGot[1];
    }

    function getTokenPath(address _token)
        public
        view
        returns (address[] memory)
    {
        address[] memory path = new address[](2);
        path[0] = pancakeRouter.WETH();
        path[1] = _token;
        return path;
    }

    function getTokenPathReverse(address _token)
        public
        view
        returns (address[] memory)
    {
        address[] memory path = new address[](2);
        path[0] = _token;
        path[1] = pancakeRouter.WETH();
        return path;
    }

    function getEtfPrice(uint256 _amount) public view returns (uint256) {
        uint256 fee = (_amount * owner_fee) / 10**fee_percentage_decimals;

        uint256[2] memory prices = getTokenPrice();
        uint256 token1share = (prices[0] * 100) / (prices[0] + prices[1]);
        uint256 token2share = 100 - token1share;
        uint256 token1value = ((_amount * token1share) / 100) - (fee / 2);
        uint256 token2value = ((_amount * token2share) / 100) - (fee / 2);

        uint256 tokensGot1 = getMinimumTokens(
            token1value,
            getTokenPath(token1)
        );
        uint256 tokensGot2 = getMinimumTokens(
            token2value,
            getTokenPath(token2)
        );

        uint256 etfShare1 = (tokensGot1 / token1amount) * 100;
        uint256 etfShare2 = (tokensGot2 / token2amount) * 100;

        if (etfShare1 <= etfShare2) {
            return (etfShare1 / 100);
        } else {
            return (etfShare2 / 100);
        }
    }

    function change_minimum_amount(uint256 _minimum_amount) public onlyOwner {
        minimum_amount = _minimum_amount;
    }

    function withdraw() public onlyOwner {
        (bool success, ) = msg.sender.call{value: address(this).balance}("");
        require(success, "transfer failed");
    }
}
